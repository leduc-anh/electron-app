    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],

    /* ðŸ”‘ buá»™c náº¡p typings React */
    "types": ["react", "react-dom"],

    /* ðŸ”‘ Ä‘áº£m báº£o tsc tÃ¬m trong node_modules/@types */
